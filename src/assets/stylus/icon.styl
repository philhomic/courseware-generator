@font-face 
  font-family: 'cw'
  src:  url('../fonts/cw.eot');
  src:  url('../fonts/cw.eot') format('embedded-opentype'),
    url('../fonts/cw.ttf') format('truetype'),
    url('../fonts/cw.woff') format('woff'),
    url('../fonts/cw.svg') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'cw' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-copy:before {
  content: "\e900";
}
.icon-delete:before {
  content: "\e901";
}
.icon-edit:before {
  content: "\e902";
}
.icon-key:before {
  content: "\e98d";
}
.icon-cross:before {
  content: "\ea0f";
}
.icon-checkmark:before {
  content: "\ea10";
}

